  top: "score"  
  name: "concat_score"
  type: "Concat"
  concat_param {
    axis: 1
  }
}
layer {
  bottom: "score"
  bottom: "label"
  name: "loss"
  type: "SigmoidCrossEntropyLoss"
  loss_weight: 1
  include: { phase: TRAIN }
}

layer {
  name: "attr_prob"
  type: "Sigmoid"
  bottom: "score"
  top: "attr_prob"
}
layer {
  name: "error"
  type: "Python"
  bottom: "attr_prob"
  bottom: "label"
  top: "error"
  python_param {
    module: "layers.multilabel_err"
