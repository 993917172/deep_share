name: "VGG_CNN_M"
layer {
  name: "data"
  type: "Python"
  top: "data"
  top: "label"
  python_param {
    module: "layers.att_data"
    layer: "AttributeData"
    param_str: "{num_classes: 2, stage: TRAIN, class_list: [0, 1]}" 
  }
  include: { phase: TRAIN }
}
layer {
  name: "data"
  type: "Python"
  top: "data"
  top: "label"
  python_param {
    module: "layers.att_data"
    layer: "AttributeData"
    param_str: "{num_classes: 2, stage: VAL, class_list: [0, 1]}" 
  }
  include: { phase: TEST }
}
layer {
  bottom: "data"
  top: "basis1"
  name: "basis1"
  type: "Convolution"
  param {
    name: "basis1_w"
    lr_mult: 1
    decay_mult: 1
  }
  param {
    name: "basis1_b"
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 15  # number of basis
    kernel_size: 7
    stride: 2
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }       
  }
}
layer {
  bottom: "basis1"
  top: "conv1-1"
  name: "conv1-1"
  type: "Convolution"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  convolution_param {
    num_output: 48
    kernel_size: 1
    stride: 1
    bias_term: false
    weight_filler {
      type: "gaussian"
      std: 0.1
    }
  }
}
layer {
  bottom: "basis1"
  top: "conv1-2"
  name: "conv1-2"
  type: "Convolution"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  convolution_param {
    num_output: 48
    kernel_size: 1
    stride: 1
    bias_term: false
    weight_filler {
      type: "gaussian"
      std: 0.1
    }
  }
}
layer {
  bottom: "conv1-1"
  top: "conv1-1"
  name: "relu1-1"
  type: "ReLU"
}
layer {
  bottom: "conv1-2"
  top: "conv1-2"
  name: "relu1-2"
  type: "ReLU"
}
layer {
  bottom: "conv1-1"
  top: "norm1-1"
  name: "norm1-1"
  type: "LRN"
  lrn_param {
    local_size: 5
    alpha: 0.0005
    beta: 0.75
    k: 2
  }
}
layer {
  bottom: "conv1-2"
  top: "norm1-2"
  name: "norm1-2"
  type: "LRN"
  lrn_param {
    local_size: 5
    alpha: 0.0005
    beta: 0.75
    k: 2
  }
}
layer {
  bottom: "norm1-1"
  top: "pool1-1"
  name: "pool1-1"
  type: "Pooling"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
layer {
  bottom: "norm1-2"
  top: "pool1-2"
  name: "pool1-2"
  type: "Pooling"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
layer {
